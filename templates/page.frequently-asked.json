    "rich_text_Pixw9N": {
      "type": "rich-text",
      "blocks": {
        "heading_dmWjq6": {
          "type": "heading",
          "settings": {
            "heading": "<strong>Thrilljoy Collectible Editions: What You Need to Know</strong>",
            "use_h1": false,
            "heading_size": "h2"
          }
        },
        "text_rXyHHg": {
          "type": "text",
          "settings": {
            "text": "<p>At Thrilljoy, we love keeping things exciting—so when you grab a PIX! collectible, you’re not just getting an awesome figure, you’re also getting the<em>thrill of the chase</em>!</p>"
          }
        }
      },
